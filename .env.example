# Data Access Tokens
SPARK_API_TOKEN=your_spark_token_here
SSC_API_TOKEN=your_ssc_token_here
ABCD_USERNAME=your_abcd_username
ABCD_PASSWORD=your_abcd_password
UKB_APPLICATION_ID=your_ukb_app_id

# Data Use Agreements - set to "true" after signing
SPARK_DUA_SIGNED=false
SSC_DUA_SIGNED=false
ABCD_DUA_SIGNED=false
UKB_DUA_SIGNED=false

# Data paths (override defaults)
# DATA_ROOT=/path/to/data
# RAW_DATA_DIR=/path/to/data/raw
# PROCESSED_DATA_DIR=/path/to/data/processed

# Compute resources
N_JOBS=4
GPU_ENABLED=false

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/pipeline.log

# Random seed for reproducibility
RANDOM_SEED=42

# API keys for ontology/pathway services
KEGG_API_KEY=
REACTOME_API_KEY=

# Email notifications (optional)
NOTIFY_EMAIL=
SMTP_SERVER=
SMTP_PORT=587